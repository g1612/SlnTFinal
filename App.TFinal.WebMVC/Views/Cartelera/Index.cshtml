@model IEnumerable<App.TFinal.Models.Cartelera>

@*@{
        ViewBag.Title = "Index";
    }*@

<h2>Mantenimiento de Cartelera</h2>
@Messages.Message("createMessage", "Nuevo", "success", "El registro se creó satisfactoriamente")
@Messages.Message("deleteMessage", "Eliminar", "danger", "El registro se eliminó satisfactoriamente")
@Messages.Message("editMessage", "Editar", "primary", "El registro se editó satisfactoriamente")

<p>
    <a href="#" onclick="appVentas.getModal('@Url.Action("Create")');" data-toggle="modal"
       data-target="#modal-container" class="btn btn-success">
        <span class="fa fa-plus"></span> Crear Cartelera
    </a>
</p>

<div id="carteleraList">
    @Html.Partial("_List")
</div>


 

@Modal.GetModal("Mantenimiento de Cartelera")

